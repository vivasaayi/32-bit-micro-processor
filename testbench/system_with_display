    %addi 1, 0, 32;
    %assign/vec4 v0x122e59f40_0, 0;
T_11.1 ;
    %load/vec4 v0x122e5a5c0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_11.2, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x122e5a0e0_0, 0;
    %load/vec4 v0x122e59eb0_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz  T_11.4, 5;
    %vpi_call 2 48 "$display", "Frame %d completed", v0x122e59eb0_0 {0 0 0};
    %load/vec4 v0x122e59eb0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x122e59eb0_0, 0;
T_11.4 ;
    %jmp T_11.3;
T_11.2 ;
    %load/vec4 v0x122e5a420_0;
    %nor/r;
