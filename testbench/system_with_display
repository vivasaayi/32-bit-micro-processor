    %load/vec4 v0x122e5a0e0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x122e5a0e0_0, 0;
T_11.6 ;
T_11.3 ;
    %jmp T_11;
    .thread T_11;
    .scope S_0x122e1d7c0;
T_12 ;
    %vpi_call 2 58 "$display", "=== Enhanced Microprocessor System Test ===" {0 0 0};
    %vpi_call 2 59 "$dumpfile", "system_with_display.vcd" {0 0 0};
    %vpi_call 2 60 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x122e1d7c0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x122e5a010_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x122e5a010_0, 0, 1;
    %vpi_call 2 67 "$display", "System started:" {0 0 0};
    %vpi_call 2 68 "$display", "  CPU running: %b", v0x122e59d50_0 {0 0 0};
    %vpi_call 2 69 "$display", "  Display active: %b", v0x122e59de0_0 {0 0 0};
    %delay 2935228928, 7;
    %vpi_call 2 74 "$display", "Test completed after 5 VGA frames" {0 0 0};
    %vpi_call 2 75 "$display", "Final status:" {0 0 0};
    %vpi_call 2 76 "$display", "  CPU running: %b", v0x122e59d50_0 {0 0 0};
    %vpi_call 2 77 "$display", "  Display active: %b", v0x122e59de0_0 {0 0 0};
    %vpi_call 2 78 "$display", "  Last VGA RGB: R=%d G=%d B=%d", v0x122e5a4f0_0, v0x122e5a350_0, v0x122e5a240_0 {0 0 0};
    %vpi_call 2 80 "$finish" {0 0 0};
    %end;
    .thread T_12;
    .scope S_0x122e1d7c0;
T_13 ;
    %wait E_0x122e3fca0;
    %load/vec4 v0x122e5a170_0;
    %nor/r;
    %flag_set/vec4 9;
    %flag_get/vec4 9;
    %jmp/0 T_13.2, 9;
    %load/vec4 v0x122e5a4f0_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/1 T_13.4, 5;
    %flag_mov 9, 5;
    %load/vec4 v0x122e5a350_0;
    %pad/u 32;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %flag_or 5, 9;
T_13.4;
    %flag_get/vec4 5;
    %jmp/1 T_13.3, 5;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x122e5a240_0;
    %pad/u 32;
    %cmp/u;
    %flag_get/vec4 5;
    %or;
T_13.3;
    %and;
T_13.2;
    %flag_set/vec4 8;
    %jmp/0xz  T_13.0, 8;
    %load/vec4 v0x122e59f40_0;
    %pushi/vec4 100, 0, 32;
    %mod/s;
    %cmpi/e 0, 0, 32;
    %flag_get/vec4 4;
    %jmp/0 T_13.7, 4;
    %load/vec4 v0x122e5a0e0_0;
    %pushi/vec4 100, 0, 32;
    %mod/s;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
T_13.7;
    %flag_set/vec4 8;
    %jmp/0xz  T_13.5, 8;
T_13.5 ;
T_13.0 ;
    %jmp T_13;
    .thread T_13;
